   "tokenColors": [
      {
         "name": "Comment",
         "scope": ["comment", "punctuation.definition.comment"],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#c0c0a2"
         }
      },
      {
         "name": "Variables",
         "scope": ["variable", "string constant.other.placeholder"],
         "settings": {
            "foreground": "#ffaa41"
         }
      },
      {
         "name": "Colors",
         "scope": ["constant.other.color"],
         "settings": {
            "foreground": "#ffffff"
